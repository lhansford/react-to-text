{
  "name": "react-to-text",
  "version": "1.1.0",
  "description": "Convert react components to plain text without any HTML markup",
  "keywords": [
    "react",
    "utility",
    "typescript",
    "conversion"
  ],
  "homepage": "https://github.com/lhansford/react-to-text",
  "bugs": {
    "url": "https://github.com/lhansford/react-to-text/issues",
    "email": "mail@lukehansford.me"
  },
  "author": "Luke Hansford",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rollup --config ./config/rollup.config.js",
    "lint": "eslint src --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "yarn lint --fix",
    "start": "rollup --config ./config/rollup.config.js -w",
    "test": "jest --config ./config/jest.config.js",
    "test:watch": "jest --watchAll",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-transform-async-to-generator": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "babel-jest": "^28.1.3",
    "babel-loader": "^9.1.0",
    "eslint": "^8.11.0",
    "eslint-config-lukehansford-react": "^1.1.0",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "jest": "^27.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rollup": "^2.70.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
